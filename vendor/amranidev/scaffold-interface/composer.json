{
    "name": "amranidev/scaffold-interface",
    "description": "A Smart CRUD Generator For Laravel",
    "homepage": "http://github.com/amranidev/scaffold-interface",
    "keywords": ["laravel", "framework", "files", "scaffod", "CRUD", "crud generator", "crud generators", "scaffoldinterface", "Scaffolding", "Scaffold", "laravel bootstrap", "laravel materialize", "Laravel 5.1", "Laravel 5.2", "amranidev", "Amrani Houssain", "amrnaihoussain", "smart crud generator", "smart generator"],
    "license": "MIT",
    "authors": [{
        "name": "Amrani Houssain",
        "email": "amranidev@gmail.com"
    }],
    "require": {
        "php": ">=7.0",
        "Amranidev/Ajaxis": "3.0.*",
        "spatie/laravel-permission": "^2.7",
        "pusher/pusher-http-laravel": "^4.0.0",
        "gajus/dindent": "2.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0|~5.0|~6.0",
        "orchestra/testbench": "~3.5.0"
    },
    "autoload": {
        "psr-4": {
            "Amranidev\\ScaffoldInterface\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Amranidev\\ScaffoldInterface\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.8.x-dev"
        },
        "laravel": {
            "providers": [
                "Amranidev\\ScaffoldInterface\\ScaffoldInterfaceServiceProvider"
            ]
        }
    }
}